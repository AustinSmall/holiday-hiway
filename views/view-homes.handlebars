<ol class="house-list">
	{{#each house as |house|}}
		<section>
			<article class="post">
				<div class="title">
					<a href="{{post.post_url}}" target="_blank">{{post.title}}</a>

				</div>
				<div class="meta">
					Posted by
					{{post.user.username}}
					on
					{{post.created_at}}
					{{!-- |
				<a href="/post/{{id}}">{{comments.length}} comment(s)</a> --}}
				</div>
				<div>
					<p>{{post.description}}</p>
				</div>
			</article>
		</section>
	{{/each}}
</ol>