<div class="container">
	<div class="dashboard">
		<p>Parade of Holiday Homes!</p>
	</div>
	<section class="dash-buttons">
        <ol class="house-list">
            {{#each houses as |house|}}
                <article>
                    <div>
                        <h3>{{house.address_1}}, {{house.city}}, {{house.state}}, {{house.zipcode}}</h3>

                    </div>
                    <div class="meta">
                        Posted by
                        {{house.owner_name}}

                    </div>
                    <div>
                        <p>{{house.photo}}</p>
                    </div>
                </article>
            {{/each}}
        </ol>
	</section>
</div>