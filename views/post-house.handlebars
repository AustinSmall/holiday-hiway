 <div class="row justify-space-between-md">
    <div class="login-card">
    <h2 class="page-title">Showcase Your Home</h2>
    <form class="form login-form" id="new-house">
    <div class="form-group">
        <label for="username-signup">Username:</label>
        <input class="form-input" type="text" id="username" name="username-signup" />
    </div>
    <div class="form-group">
        <label for="Address1">Address 1:</label>
        <input class="form-input" type="text" id="address1", name="Address1" />
    </div>
    <div class="form-group">
        <label for="Address2">Address 2:</label>
        <input class="form-input" type="text" id="address2", name="Address2" />
    </div>
    <div class="form-group">
        <label for="city">City:</label>
        <input class="form-input" type="text" id="city", name="city" />
    </div>
     <div class="form-group">
        <label for="State">State:</label>
        <input class="form-input" type="text" id="state", name="State" />
    </div>
     <div class="form-group">
        <label for="zipcode">Zip Code:</label>
        <input class="form-input" type="text" id="zipcode", name="zipcode" />
    </div>
    <div class="form-group">
        <label for="photo">House Photo URL:</label>
        <input class="form-input" type="text" id="photo", name="photo" />
    </div>
    <div class="form-group">
        <button class="btn btn-danger" type="submit">Submit House</button>
    </div>
    </form>
 </div>
 <script src="/javascript/add-house.js"></script>
 <script src="/javascript/add-photo.js"></script>
        
